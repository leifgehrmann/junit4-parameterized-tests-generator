{
  "name": "junit4-parameterized-tests-generator",
  "version": "0.0.1",
  "description": "Script to generate parameterized JUnit4 tests for Test Runners that do not support parameterized tests",
  "main": "generateParameterizedTests.js",
  "scripts": {
    "lint": "eslint \"generateParameterizedTests.js\" \"tests/**/*.js\"",
    "format": "prettier --write\"generateParameterizedTests.js\" \"tests/**/*.js\"",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leifgehrmann/junit4-parameterized-tests-generator.git"
  },
  "keywords": [
    "java",
    "junit4",
    "parameterized",
    "tests"
  ],
  "author": "Leif Gehrmann",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/leifgehrmann/junit4-parameterized-tests-generator/issues"
  },
  "homepage": "https://github.com/leifgehrmann/junit4-parameterized-tests-generator#readme",
  "devDependencies": {
    "dir-compare": "^2.4.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "recursive-copy": "^2.0.11"
  }
}
